<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/navigation"
    app:startDestination="@id/titleFragment">

    <fragment
        android:id="@+id/detailFragment"
        android:name="com.example.marswebservicecontact.detailFragment.DetailFragment"
        android:label="DetailFragment">
        <argument
            android:name="selectedProperty"
            app:argType="com.example.marswebservicecontact.network.MarsClass"/>
    </fragment>
    <fragment
        android:id="@+id/titleFragment"
        android:name="com.example.marswebservicecontact.titleFragment.TitleFragment"
        android:label="TitleFragment" >
        <action
            android:id="@+id/action_titleFragment_to_detailFragment"
            app:destination="@id/detailFragment" />

    </fragment>
</navigation>